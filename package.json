{
  "name": "casa-esfera",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "backup:dconfig": "cp docker-compose.yml docker-compose.yml.backup 2>/dev/null || :",
    "rm:dconfig": "pnpm run backup:dconfig && rm -f docker-compose.yml",
    "cp:dev": "pnpm run rm:dconfig && cp build/development/docker-compose.yml.example docker-compose.yml",
    "cp:prod": "pnpm run rm:dconfig && cp build/production/docker-compose.yml.example docker-compose.yml",
    "cp:locally": "pnpm run rm:dconfig && cp build/locally/docker-compose.yml.example docker-compose.yml",
    "mongo:up": "pnpm run cp:locally && docker compose up -d mongo",
    "down": "docker compose down",
    "up": "docker compose up -d",
    "start:dev": "pnpm run cp:dev && docker compose up -d",
    "start:prod": "pnpm run cp:prod && docker compose up -d",
    "start:locally": "pnpm run cp:locally && pnpm run mongo:up && next dev",
    "dev0": "next dev",
    "dev": "pnpm run start:locally",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.19",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "mongoose": "^8.4.1",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "typescript": "^5"
  }
}
